<div id="landingPage">
  <div class="container">
    <form role="form" class="col-md-8 col-md-offset-2">
      <h2>Registrate</h2>
      <div class="form-group username">
        <label for="username-txt">Nombre de Usuario:</label>
        <input type="text" class="form-control" id="username-txt" data-field="username"placeholder="nombre de usuario">
      </div>
      <div class="form-group password">
        <label for="password-txt">Contraseña:</label>
        <input type="password" class="form-control" id="password-txt" data-field="password"placeholder="contrasena">
      </div>
      <div class="form-group repeat-password">
        <label for="password2-txt">Repetir Contraseña:</label>
        <input type="password" class="form-control" id="password2-txt" data-field="repeatpassword"placeholder="contraseña">
      </div>
      <div class="form-group email">
        <label for="email-txt">Correo Electronico:</label>
        <input type="email" class="form-control" id="email-txt" data-field="email" placeholder="email">
      </div>
      <div class="form-group name">
        <label for="name-txt">Nombre Completo:</label>
        <input type="text" class="form-control" id="name-txt" placeholder="Nombre Completo" data-field="name" >
      </div>
      <div class="form-group birthdate">
        <label for="birthdate-date">Fecha de Nacimiento:</label>
        <input type="date" class="form-control" id="birthdate-date" data-field="birthdate"placeholder="Fecha de Nacimiento">
      </div>
      <div class="form-group country">
        <label for="country-select">Pais:</label>
        <select name="pais" id="country-select" data-field="country"></select>
      </div>
      <button type="submit" class="btn btn-success" id="submitButton">Registrarse</button>
    </form>
  </div>
</div>